 cat geno/c3h/93133_N1_arc_filtered.nodMat geno/c3h/89985_N1_arc_filtered.nodMat | awk '{print $1"	"$2}' FS="[_,]" | sort -k 1,1 -k2,2n -u > ma/c3h/c3h.arc_filtered_control_sites.pos ;  cat geno/c3h/93133_N1_arc_filtered.nodMat geno/c3h/89985_N1_arc_filtered.nodMat | awk '{print $1"	"$2"	"$2+1}' FS="[_,]" | sort -k 1,1 -k2,2n -u > ma/c3h/c3h.arc_filtered_control_sites.bed ;  bgzip -c ma/c3h/c3h.arc_filtered_control_sites.bed > ma/c3h/c3h.arc_filtered_control_sites.bed.gz 